<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="zaliczenieMaui.ProjectDetailsPage"
             Title="">
    <ScrollView>
        <StackLayout Padding="20">
            <Label Text="Project Name" FontSize="Medium"/>
            <Label x:Name="projectNameLabel" FontSize="Large" FontAttributes="Bold"/>
            <Label Text="Project Description" FontSize="Medium"/>
            <Label x:Name="projectDescriptionLabel" FontSize="Small"/>
        </StackLayout>
    </ScrollView>
</ContentPage>
